{
  "name": "haotab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "tabs manage tool",
  "content_scripts": [
  ],
  "offline_enabled": true,
  "permissions":[
    "tabs",
    "bookmarks",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "background":{
    "scripts":["js/background.js"]
  }
}